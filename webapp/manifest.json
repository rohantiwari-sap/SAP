{
    "_version": "1.32.0",
    "sap.app": {
        "id": "rt.sap.ui51",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "ach": "ach",
        "dataSources": {
            "mainService": {
                "uri": "/V2/Odata/Odata.svc/",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0",
                    "localUri": "localService/metadata.xml"
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "sap-icon://task",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": false,
        "rootView": {
            "viewName": "rt.sap.ui51.view.main",
            "type": "XML",
            "async": true,
            "id": "main"
        },
        "dependencies": {
            "minUI5Version": "1.66.0",
            "libs": {
                "sap.ui.core": {},
                "sap.m": {},
                "sap.ui.layout": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "rt.sap.ui51.i18n.i18n"
                }
            },
            "": {
                "dataSource": "mainService",
                "preload": true
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "async": true,
                "viewPath": "rt.sap.ui51.view",
                "controlAggregation": "pages",
                "controlId": "viewappid",
                "clearControlAggregation": false
            },
            "routes": [
                {
                    "name": "Routemain",
                    "pattern": "Routemain",
                    "target": [
                        "Targetmain"
                    ]
                },
                {
                    "name": "Tiles",
                    "pattern": "",
                    "target": [
                        "Tiles"
                    ]
                },
                {
                    "name": "First",
                    "pattern": "First",
                    "target": [
                        "countdown"
                    ]
                }
            ],
            "targets": {
                "Targetmain": {
                    "viewType": "XML",
                    "transition": "slide",
                    "controlId": "viewappid",
                    "clearControlAggregation": false,
                    "viewId": "main",
                    "viewName": "main"
                },
                "Tiles": {
                    "viewType": "XML",
                    "transition": "slide",
                    "controlId": "viewappid",
                    "clearControlAggregation": false,
                    "viewId": "Tiles",
                    "viewName": "Tiles"
                },
                "countdown": {
                    "viewType": "XML",
                    "transition": "slide",
                    "controlId": "viewappid",
                    "clearControlAggregation": false,
                    "viewId": "countdown.Countdown",
                    "viewName": "countdown.Countdown"
                }
            }
        }
    }
}